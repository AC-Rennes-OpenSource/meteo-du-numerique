{
  "name": "MeteoDuNumerique",
  "version": "1.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start-dev": "yarn env:development && react-native start",
    "build-android": "yarn env:prod && cd android && ./gradlew assembleRelease",
    "build-ios": "yarn env:prod && react-native run-ios --configuration Release",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "env:development": "node scripts/set-environment.js development",
    "env:prod": "node scripts/set-environment.js prod"
  },
  "dependencies": {
    "@eva-design/eva": "^2.0.0",
    "@react-native-firebase/app": "^12.9.0",
    "@react-native-firebase/messaging": "^12.9.0",
    "@ui-kitten/components": "^5.0.0",
    "@ui-kitten/eva-icons": "^5.0.0",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-svg": "^12.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-react": "^7.20.1",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^16"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
